{
    "name": "prasanna/bagisto-weight-shipping",
    "type": "bagisto-package",
    "version": "2.0.2",
    "description": "Weight based shipping package for Bagisto - Calculate shipping costs based on total cart weight",
    "keywords": ["bagisto", "laravel", "php", "weight-based", "shipping", "ecommerce"],
    "homepage": "https://github.com/prasanna/bagisto-weight-shipping",
    "license": "MIT",
    "authors": [
        {
            "name": "Prasanna",
            "email": "prasanna@example.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "laravel/framework": "^11.0",
        "webkul/bagisto": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Prasanna\\WeightShipping\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Prasanna\\WeightShipping\\Providers\\WeightShippingServiceProvider"
            ]
        },
        "bagisto": {
            "providers": [
                "Prasanna\\WeightShipping\\Providers\\ModuleServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
